  "scripts": {
    "start": "export SET NODE_OPTIONS=--openssl-legacy-provider && webpack serve --config build-utils/webpack.config.js --env env=dev",
    "start:dev": "pm2 start npm --name 'SSC Stream' --node-args='--max-old-space-size=8192' -- run start",
    "start:prod": "export SET NODE_OPTIONS=--openssl-legacy-provider && webpack serve --config build-utils/webpack.config.js --env env=prod",
    "build": "webpack --config build-utils/webpack.config.js --env env=${VENDOR}",
    "build:atan": "VENDOR=atan BUILD='./build_mkstream_atan' npm run build",
    "build:production": "VENDOR=prod npm run build",
    "build:analyze": "npm run build -- --env addon=bundleanalyze",
    "test": "echo \"Error: no test specified\" && exit 0"
  },
